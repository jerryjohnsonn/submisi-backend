{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style":"off",
    "eol-last":"off",
    "no-console":"off",
    "no-trailing-spaces":"off",
    "max-len":"off",
    "no-unneeded-ternary":"off",
    "object-curly-newline":"off"
  }
}
